#: plugin.py:536
msgid "Switch 4/9"
msgstr "Schalter 4/9"

#: plugin.py:537
msgid "Pause"
msgstr "Pause"

#: plugin.py:600
msgid "CH+/CH- : countdown to next screen in secs (3-30)\n"
msgstr "CH+/CH- : Countdown zum nächsten Bildschirm in Sekunden (3-30)\n"

#: plugin.py:601
msgid "Green   : play Mosaic\n"
msgstr "Grün   : Mosaik Abspielen\n"

#: plugin.py:602
msgid "Yellow  : pause Mosaic\n"
msgstr "Gelb  : Pausiere Mosaik\n"

#: plugin.py:603
msgid "Blue    : toggle 9/4 screens\n"
msgstr "Blau    : Umschalten 9/4 Bildschirme\n"

#: plugin.py:604
msgid "1-9(4)  : switch to screen 1-9(4) and leave\n"
msgstr "1-9(4)  : zu Bildschirm 1-9(4) wechseln und verlassen\n"

#: plugin.py:605
msgid "OK      : switch to current screen and leave\n"
msgstr "OK      : zum aktuellen Bildschirm wechseln und verlassen\n"

#: plugin.py:606
msgid "Exit    : leave to previously service\n"
msgstr "Exit    : zum vorherigen Dienst gehen\n"

#: plugin.py:607
msgid "Help    : this help"
msgstr "Hilfe : diese Hilfe"

msgid "1-9(4)  : switch to screen 1-9(4) and leave\n"
msgstr "1-9(4)  : passa alla schermata 1-9(4) aed esci\n"

#: plugin.py:605
msgid "OK      : switch to current screen and leave\n"
msgstr "OK      : passa alla schermata corrente e esci\n"

#: plugin.py:606
msgid "Exit    : leave to previously service\n"
msgstr "Esci    : abbandonare il servizio precedente\n"

